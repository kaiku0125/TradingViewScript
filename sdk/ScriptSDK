//@version=5

var VERSION_NAME = "v1.0"

library(title = "ScriptSDK", overlay =  true) 
var color DEFALUT = color.white
var color WHITE = color.white
var color BLACK = color.black


export getColor(int code) =>
    color output = switch code
        0 => WHITE
        1 => BLACK
        =>
            DEFALUT

    output



// export getDebugTable() =>
//     table output






export getSDKVersion() =>
    VERSION_NAME